<?xml version="1.0" encoding="UTF-8"?>
<includes>
	<default type="image">
		<texture />
		<align>center</align>
        <aligny>center</aligny>
    </default>
	<default type="multiimage">
		<aspectratio align="center">scale</aspectratio>
		<fadetime>800</fadetime>
		<timeperimage>5000</timeperimage>
		<randomize>true</randomize>
	</default>
	<default type="textbox">
		<align>left</align>
		<aligny>center</aligny>
		<textcolor>white</textcolor>
		<font>font13</font>
		<autoscroll delay="3000" time="1000" repeat="10000">true</autoscroll>
		<wrapmultiline>true</wrapmultiline>
	</default>
	<default type="label">
		<label />
		<align>left</align>
		<aligny>center</aligny>
		<font>font13</font>
		<textcolor>white</textcolor>
		<disabledcolor>40FFFFFF</disabledcolor>
		<selectedcolor>$VAR[Button_Selected]</selectedcolor>
		<scroll>false</scroll>
		<scrollspeed>40</scrollspeed>
	</default>
	<default type="fadelabel">
		<align>left</align>
		<font>font13</font>
		<textcolor>white</textcolor>
		<disabledcolor>40FFFFFF</disabledcolor>
	</default>
	<default type="scrollbar">
		<texturesliderbackground colordiffuse="$VAR[scrollbar_panel]">colors/white.png</texturesliderbackground>
		<texturesliderbar colordiffuse="$VAR[scrollbar_slider_nf]">colors/white.png</texturesliderbar>
		<texturesliderbarfocus colordiffuse="$VAR[scrollbar_slider_fo]">colors/white.png</texturesliderbarfocus>
		<textureslidernib />
		<textureslidernibfocus />
		<width>5</width>
		<showonepage>false</showonepage>
		<orientation>vertical</orientation>
		<onup>60</onup>
		<ondown>60</ondown>
	</default>
	<default type="button">
		<texturefocus colordiffuse="$VAR[Button_Panel_FO]">colors/white.png</texturefocus>
		<texturenofocus colordiffuse="$VAR[Button_Panel_NF]">colors/white.png</texturenofocus>
		<height>50</height>
		<width>100%</width>
		<font>font27</font>
		<focusedcolor>$VAR[Button_Text_FO]</focusedcolor>
		<textcolor>$VAR[Button_Text_NF]</textcolor>
		<disabledcolor>40FFFFFF</disabledcolor>
		<invalidcolor>FFD32F2F</invalidcolor>
		<textoffsetx>40</textoffsetx>
		<aligny>center</aligny>
		<pulseonselect>no</pulseonselect>
	</default>
	
	
			<default type="togglebutton">
				<height>50</height>
				<texturefocus border="21" colordiffuse="$VAR[Button_Panel_FO]">buttons/button-fo.png</texturefocus>
				<texturenofocus border="21" colordiffuse="$VAR[Button_Panel_NF]">buttons/button-nofo.png</texturenofocus>
				<alttexturefocus border="21" colordiffuse="$VAR[Button_Panel_FO]">buttons/button-fo.png</alttexturefocus>
				<alttexturenofocus border="21" colordiffuse="$VAR[Button_Panel_NF]">buttons/button-nofo.png</alttexturenofocus>
				<focusedcolor>$VAR[Button_Text_FO]</focusedcolor>
				<textcolor>$VAR[Button_Text_NF]</textcolor>
				<font>font13</font>
				<disabledcolor>40FFFFFF</disabledcolor>
				<pulseonselect>no</pulseonselect>
			</default>
	
	<default type="radiobutton">
		<height>50</height>
		<top>0</top>
		<aligny>center</aligny>
		<textoffsetx>40</textoffsetx>
		<radiowidth>45</radiowidth>
		<radioheight>35</radioheight>
		<textureradioonfocus colordiffuse="$VAR[Button_Selected]">buttons/radio_on_fo.png</textureradioonfocus>
		<textureradioonnofocus colordiffuse="$VAR[Button_Selected]">buttons/radio_on_fo.png</textureradioonnofocus>
		<textureradioofffocus colordiffuse="$VAR[Button_Text_NF]">buttons/radio_off_fo.png</textureradioofffocus>
		<textureradiooffnofocus colordiffuse="$VAR[Button_Text_NF]">buttons/radio_off_fo.png</textureradiooffnofocus>
		<texturefocus colordiffuse="$VAR[Button_Panel_FO]">colors/white.png</texturefocus>
		<texturenofocus colordiffuse="$VAR[Button_Panel_NF]">colors/white.png</texturenofocus>
		<textureradioondisabled colordiffuse="disabled">buttons/radio_on.png</textureradioondisabled>
		<textureradiooffdisabled colordiffuse="disabled">buttons/radio_off.png</textureradiooffdisabled>
		<font>font27</font>
		<focusedcolor>$VAR[Button_Text_FO]</focusedcolor>
		<textcolor>$VAR[Button_Text_NF]</textcolor>
		<disabledcolor>40FFFFFF</disabledcolor>
		<invalidcolor>FFD32F2F</invalidcolor>
		<selectedcolor>$VAR[Button_Selected]</selectedcolor>
		<pulseonselect>no</pulseonselect>
	</default>
	
	<default type="spincontrol">
		<textureup colordiffuse="disabled">buttons/spinctrl_up.png</textureup>
		<texturedown colordiffuse="disabled">buttons/spinctrl_down.png</texturedown>
		<textureupfocus>buttons/spinctrl_up.png</textureupfocus>
		<texturedownfocus>buttons/spinctrl_down.png</texturedownfocus>
		<textureupdisabled colordiffuse="disabled">buttons/spinctrl_up.png</textureupdisabled>
		<texturedowndisabled colordiffuse="disabled">buttons/spinctrl_down.png</texturedowndisabled>
		<align>right</align>
		<width>77</width>
		<height>50</height>
		<font>font13</font>
		<reverse>yes</reverse>
		<focusedcolor>$VAR[Button_Text_FO]</focusedcolor>
		<textcolor>$VAR[Button_Text_NF]</textcolor>
		<disabledcolor>40FFFFFF</disabledcolor>
		<subtype>text</subtype>
		<textoffsetx>40</textoffsetx>
		<textoffsety>0</textoffsety>
		<pulseonselect>no</pulseonselect>
	</default>
	<default type="spincontrolex">
		<height>50</height>
		<spinwidth>32</spinwidth>
		<spinheight>60</spinheight>
		<texturefocus colordiffuse="$VAR[Button_Panel_FO]">colors/white.png</texturefocus>
		<texturenofocus colordiffuse="$VAR[Button_Panel_NF]">colors/white.png</texturenofocus>
		<textureup colordiffuse="disabled">buttons/spinctrl_up.png</textureup>
		<texturedown colordiffuse="disabled">buttons/spinctrl_down.png</texturedown>
		<textureupfocus>buttons/spinctrl_up.png</textureupfocus>
		<texturedownfocus>buttons/spinctrl_down.png</texturedownfocus>
		<textureupdisabled colordiffuse="disabled">buttons/spinctrl_up.png</textureupdisabled>
		<texturedowndisabled colordiffuse="disabled">buttons/spinctrl_down.png</texturedowndisabled>
		<font>font13</font>
		<focusedcolor>$VAR[Button_Text_FO]</focusedcolor>
		<textcolor>$VAR[Button_Text_NF]</textcolor>
		<disabledcolor>40FFFFFF</disabledcolor>
		<textoffsetx>40</textoffsetx>
		<aligny>center</aligny>
		<reverse>yes</reverse>
		<pulseonselect>no</pulseonselect>
	</default>
	
	<default type="progress">
		<texturebg colordiffuse="$VAR[Widget_Progress_Remain]" border="2">colors/white.png</texturebg>
		<midtexture colordiffuse="$VAR[Widget_Progress_Watched]" border="2">colors/white.png</midtexture>
		<lefttexture />
		<righttexture />
		<overlaytexture />
	</default>
	
	<default type="slider">
		<height>50</height>
		<texturesliderbar border="10">buttons/slider-back.png</texturesliderbar>
		<textureslidernib>buttons/slider-nib.png</textureslidernib>
		<textureslidernibfocus colordiffuse="$VAR[Button_Panel_FO]">buttons/slider-nib.png</textureslidernibfocus>
		<orientation>horizontal</orientation>
	</default>
	<default type="sliderex">
		<height>50</height>
		<textoffsetx>40</textoffsetx>
		<aligny>center</aligny>
		<texturefocus colordiffuse="$VAR[Button_Panel_FO]">colors/white.png</texturefocus>
		<texturenofocus colordiffuse="$VAR[Button_Panel_NF]">colors/white.png</texturenofocus>
		<sliderwidth>150</sliderwidth>
		<sliderheight>28</sliderheight>
		<texturesliderbar colordiffuse="$VAR[Button_Text_FO]">buttons/slider-back.png</texturesliderbar>
		<textureslidernib colordiffuse="$VAR[Button_Text_NF]">buttons/slider-nib.png</textureslidernib>
		<textureslidernibfocus colordiffuse="$VAR[Button_Selected]">buttons/slider-nib.png</textureslidernibfocus>
		<font>font13</font>
		<focusedcolor>$VAR[Button_Text_FO]</focusedcolor>
		<textcolor>$VAR[Button_Text_NF]</textcolor>
		<disabledcolor>40FFFFFF</disabledcolor>
	</default>
	
	<default type="mover">
		<texturefocus colordiffuse="$VAR[Button_Panel_FO]">colors/white.png</texturefocus>
		<texturenofocus colordiffuse="$VAR[Button_Panel_NF]">colors/white.png</texturenofocus>
	</default>
	<default type="resize">
		<texturefocus colordiffuse="$VAR[Button_Panel_FO]">colors/white.png</texturefocus>
		<texturenofocus colordiffuse="$VAR[Button_Panel_NF]">colors/white.png</texturenofocus>
	</default>
	<default type="edit">
		<height>50</height>
		<texturefocus colordiffuse="$VAR[Button_Panel_FO]">colors/white.png</texturefocus>
		<texturenofocus colordiffuse="$VAR[Button_Panel_NF]">colors/white.png</texturenofocus>
		<font>font13</font>
		<focusedcolor>$VAR[Button_Text_FO]</focusedcolor>
		<textcolor>$VAR[Button_Text_NF]</textcolor>
		<disabledcolor>40FFFFFF</disabledcolor>
		<invalidcolor>FFD32F2F</invalidcolor>
		<textoffsetx>40</textoffsetx>
		<aligny>center</aligny>
		<pulseonselect>no</pulseonselect>
	</default>
</includes>