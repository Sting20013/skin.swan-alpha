	<include name="panel_100">
		<control type="panel" id="100">
			<description>temp use hardcoded b/w</description>
			<top>$PARAM[top]</top>
			<height>100</height>
			<width>100%</width>
			<orientation>horizontal</orientation>
			<defaultcontrol always="true">0</defaultcontrol>
			<onright>noop</onright>
			<onleft>noop</onleft>
			<ondown>10052</ondown>
			<itemlayout height="100" width="220">
				<control type="group">
					<top>30</top>
					<width>185</width>
					<height>100</height>
					<control type="group">
						<height>62</height>
						<control type="image">
							<texture border="2" colordiffuse="white">frames/frametest.png</texture>
						</control>
						<control type="label">
							<height>62</height>
							<align>center</align>
							<aligny>center</aligny>
							<font>font12</font>
							<textcolor>white</textcolor>
							<label>$INFO[ListItem.Label]</label>
						</control>
					</control>
				</control>
			</itemlayout>
			<focusedlayout height="100" width="220">
				<control type="group">
					<top>30</top>
					<width>185</width>
					<height>100</height>
					<control type="group">
						<height>62</height>
						<control type="image">
							<texture colordiffuse="white">colors/white.png</texture>
						</control>
						<control type="label">
							<height>62</height>
							<align>center</align>
							<aligny>center</aligny>
							<font>font12</font>
							<textcolor>black</textcolor>
							<label>$INFO[ListItem.Label]</label>
						</control>
					</control>
				</control>
			</focusedlayout>
			
			<nested />
		</control>
	</include>
	<include name="Ratings_Hor">
		<param name="texture" default="themoviedb" />
		<param name="rating" default="rating" />
		<param name="ratingpostfix" default="" />
		<definition>
			<include content="spacer_h">
				<param name="gap" value="10" />
			</include>
			<control type="image">
				<width>40</width>
				<texture colordiffuse="$VAR[Nav_LabelPrimaryColor]">flags/ratings/$PARAM[texture]_monochrome.png</texture>
				<aspectratio>keep</aspectratio>
				<visible>!String.IsEmpty(Container(10051).ListItem.$PARAM[rating])</visible>
			</control>
			<control type="label">
				<label>$INFO[Container(10051).ListItem.$PARAM[rating],,$PARAM[ratingpostfix]]</label>
				<width>auto</width>
				<font>debug</font>
				<textcolor>$VAR[Nav_LabelPrimaryColor]</textcolor>
				<visible>!String.IsEmpty(Container(10051).ListItem.$PARAM[rating])</visible>
			</control>
		</definition>
	</include>
	
