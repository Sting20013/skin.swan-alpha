			<left>60</left>
			<right>50</right>
			<height>100</height>
			<aligny>center</aligny>
			<align>center</align>
			<orientation>horizontal</orientation>
			<itemgap>20</itemgap>
			<usecontrolcoords>true</usecontrolcoords>
			
			<onup>699</onup>
			<ondown>699</ondown>
			
			<!-- 
			<ondown>SetProperty(CurrentPlaylistVisible,true,home)</ondown>
			<ondown>SetFocus(499)</ondown>
			-->
			
			<!-- 500	button	Select visualization
				<label>$LOCALIZE[250]</label>
				<selected>Visualisation.Enabled</selected>
			 501	button	Lock visualization to the current preset
			
			optional
			<onclick>ActivateWindow(visualisationpresetlist)</onclick>
			<visible>Visualisation.Enabled + Visualisation.HasPresets</visible>
		
			<onclick>Addon.Default.OpenSettings(xbmc.player.musicviz)</onclick>
			<visible>Visualisation.Enabled</visible>
		
			-->
		
			<control type="group" id="601">
				<width>55</width>
				<height>100</height>
		
				<control type="button" id="6011">
					<top>0</top>
					<width>45</width>
					<height>45</height>
					<label />
					<texturefocus colordiffuse="$VAR[MusicPlayerTextAccent]">buttons/osd/crlyrics.png</texturefocus>
					<texturenofocus colordiffuse="$VAR[MusicPlayerTextMainNF]">buttons/osd/crlyrics.png</texturenofocus>
					<onclick>RunScript(script.cu.lrclyrics)</onclick>
					<onleft condition="Control.IsVisible(6092)">6092</onleft>
					<onleft condition="Control.IsVisible(6093)">6093</onleft>
					<onleft condition="Control.IsVisible(6094)">6094</onleft>
					<onright condition="Control.IsVisible(603)">603</onright>
					<onright condition="!Control.IsVisible(603)">604</onright>
					<onup>500</onup>
					<ondown>500</ondown>
				</control>
				
				<control type="button" id="500">
					<top>50</top>
					<width>45</width>
					<height>45</height>
					<label />
					<texturefocus colordiffuse="$VAR[MusicPlayerTextAccent]">buttons/osd/visualisation.png</texturefocus>
