					<height>45</height>
					<label />
					<texturefocus colordiffuse="$VAR[MusicPlayerTextAccent]">buttons/osd/repeatone.png</texturefocus>
					<texturenofocus colordiffuse="$VAR[MusicPlayerTextMainNF]">buttons/osd/repeatone.png</texturenofocus>
					<onright>6011</onright>
					<onleft condition="Control.IsVisible(607)">607</onleft>
					<onleft condition="!Control.IsVisible(607)">605</onleft>
					<onup>6096</onup><ondown>6096</ondown>
					<onclick>PlayerControl(Repeat)</onclick>
					<onclick>Notification(Repeat,[B]T i t l e[/B],10,buttons/osd/repeatone.png)</onclick>
				</control>
				
