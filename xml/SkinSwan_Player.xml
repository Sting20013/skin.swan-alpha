					<texturenofocus colordiffuse="$VAR[MusicPlayerTextMainNF]">buttons/osd/repeatoff.png</texturenofocus>
					<onright>6011</onright>
					<onleft condition="Control.IsVisible(607)">607</onleft>
					<onleft condition="!Control.IsVisible(607)">605</onleft>
					<onup>6096</onup><ondown>6096</ondown>
					<onclick>PlayerControl(Repeat)</onclick>
					<onclick>Notification(Repeat,[B]D i s a b l e d[/B],10,buttons/osd/repeatoff.png)</onclick>
				</control>
				
				<control type="togglebutton" id="6096">
					<top>40</top>
					<width>45</width>
					<height>45</height>
					<label />
					<onright>500</onright>
					
					<ondown condition="Control.IsVisible(6097)">6097</ondown>
					
					<onup condition="Control.IsVisible(6092)">6092</onup><ondown condition="Control.IsVisible(6092)">6092</ondown>
					<onup condition="Control.IsVisible(6094)">6094</onup><ondown condition="Control.IsVisible(6094)">6094</ondown>
					<onup condition="Control.IsVisible(6093)">6093</onup><ondown condition="Control.IsVisible(6093)">6093</ondown>
					
